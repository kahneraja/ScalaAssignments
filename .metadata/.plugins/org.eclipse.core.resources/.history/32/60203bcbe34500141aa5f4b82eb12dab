object Research {
  def factorial(x: Int): Int =
    if (x == 0) 1 else x * factorial(x - 1)
  
  def fTailProduct(x: Int, next: Int): Int =
  	x * fTail(next)
  
  def fTail(x: Int): Int =
    if (x == 0) 1 else fTailProduct(x, x - 1)

  factorial(5)
  factorial(1)
  factorial(2)
  factorial(3)
  
  
  
  def balance(chars: List[Char]): Boolean = tre
  
  balance("()")
}